{ signal: [
  { name: "clk",           wave: "p.........." },
  { name: "address",       wave: "x2.x.2=x...", data:"addr1 addr2 addr3"},
  { name: "read",          wave: "01.0.1.0..."},
  { name: "waitrequest",   wave: "x10x.0.x..."},
  { name: "readdata",      wave: "x...2x.2=xx",data:"data1 data2 data3"}
  ],
 head:{
   text:'pipelined read transfer with fixed latency of two clock cycles',
   tick:0,
 },
 foot:{
   text:'Figure 7',
   },
  config: { hscale: 2 }
}