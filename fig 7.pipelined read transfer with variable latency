  { name: "clk",           wave: "p..........." },
  { name: "address",       wave: "x2==.==x....", data:"addr1 addr2 addr3 addr4 addr5"},
  { name: "read",          wave: "01.....0...."},
  { name: "waitrequest",   wave: "x0.10..x...."},
  { name: "readdata",      wave: "x...2===x.2x",data:"data1 data2 data3 data4 data5"},
  { name: "readdatavalid", wave: "0...1...0.10"}
 ],
